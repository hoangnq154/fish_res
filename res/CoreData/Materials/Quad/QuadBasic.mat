material quadbasic
{
    technique quadbasic
    {
        pass base
        {
            // shaders
            vertexShader = Shaders/Postprocessing/vfs_Basic.sc
            fragmentShader =  Shaders/Postprocessing/vfs_Basic.sc

            parameters
            {
                  // render state
                  renderState
                  {
                      cullFace = true
                      depthTest = false
                      depthWrite = false
                      blend = false
                      blendSrc = ONE
                      blendDst = ZERO
                  }
            }
        }
    }
}
